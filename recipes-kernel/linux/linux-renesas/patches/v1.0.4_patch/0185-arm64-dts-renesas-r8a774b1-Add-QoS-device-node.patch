From 5da29f86471b2b0ea7a8f2f700644a54af360f54 Mon Sep 17 00:00:00 2001
From: Hien Huynh <hien.huynh.px@renesas.com>
Date: Wed, 4 Mar 2020 13:22:26 +0700
Subject: [PATCH 185/268] arm64: dts: renesas: r8a774b1: Add QoS device node

This patch adds QoS device node for the R8A774B1 SoC.

Signed-off-by: Loc Phuoc Pham.Lam <loc.lam.px@renesas.com>
Signed-off-by: Hien Huynh <hien.huynh.px@renesas.com>
---
 arch/arm64/boot/dts/renesas/r8a774b1.dtsi | 5 +++++
 1 file changed, 5 insertions(+)

diff --git a/arch/arm64/boot/dts/renesas/r8a774b1.dtsi b/arch/arm64/boot/dts/renesas/r8a774b1.dtsi
index 625a858..16c63d2 100644
--- a/arch/arm64/boot/dts/renesas/r8a774b1.dtsi
+++ b/arch/arm64/boot/dts/renesas/r8a774b1.dtsi
@@ -314,6 +314,11 @@
 			#power-domain-cells = <1>;
 		};
 
+		qos@e67e0000 {
+			compatible = "renesas,qos";
+			reg = <0 0xe67e0000 0 0x10090>;
+		};
+
 		dmac0: dma-controller@e6700000 {
 			compatible =	"renesas,dmac-r8a774b1",
 					"renesas,rcar-dmac";
-- 
2.7.4

